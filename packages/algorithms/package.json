{
  "name": "@gitslither/algorithms",
  "version": "1.0.0",
  "description": "Path generation algorithms for GitSlither",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch",
    "clean": "rimraf dist",
    "lint": "oxlint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "valibot": "^0.28.1"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "vitest": "^1.3.0",
    "rimraf": "^5.0.5"
  }
}
